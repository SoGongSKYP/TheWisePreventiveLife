<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".PageOfSelfDiagnosisResult">

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/diagnosis_result_Toolbar"
        android:elevation="10dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:minHeight="?attr/actionBarSize"
        app:layout_constraintTop_toTopOf="parent"
        android:paddingTop="24dp"
        android:background="@color/colorWhite">
        <TextView
            android:id="@+id/diagnosis_result_Title_TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:text="자가 진단 결과"
            android:textSize="20dp"
            android:textColor="@color/colorDarkGrey"/>
    </androidx.appcompat.widget.Toolbar>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layout_constraintTop_toBottomOf="@id/diagnosis_result_Toolbar"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:orientation="vertical">
        <TextView
            android:id="@+id/diagnosis_result_TextView"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="결과"/>
    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>